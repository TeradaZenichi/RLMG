{
  "name": "model2",
  "version": "1.4.1",
  "hidden_dims": [256, 256, 512, 256, 128],
  "dropout": 0.1,
  "log_std_min": -5.0,
  "log_std_max": 2.0,

  "obs": {
    "normalized_inputs": true,
    "history": {
      "enabled": true,
      "window": 1,          
      "mode": "flat",        
      "stride": 1,           
      "padding": "edge"      
    }
  },
  "imitation_learning": {
    "horizon": 1250,
    "timestep": 5,
    "optimizer": { "type": "adam", "lr": 0.001, "weight_decay": 0.0 },
    "loss": { "type": "mse", "delta": 1.0 },
    "train": {
      "batch_size": 256,
      "epochs": 30,
      "shuffle": true,
      "val_split": 0.1,
      "early_stopping_patience": 15,
      "save_best_only": true,
      "grad_clip": 0.0,
      "num_workers": 0,
      "seed": 42
    }
  },

  "reinforcement_learning": {
    "horizon": 48,
    "timestep": 5,
    "nenvs": 10,
    "ndays": 50,

    "optimizer": {
      "type": "adam",
      "weight_decay": 0.0
    },

    "train": {
      "buffer_size": 1e6,
      "n_episodes": 100,
      "save_every": 1,
      "lr_actor": 5e-5,
      "lr_critic": 5e-4,
      "gamma": 0.95,
      "tau": 0.005,
      "batch_size": 512,
      "policy_delay": 2,
      "policy_noise": 0.2,
      "policy_noise_final": 0.1,
      "noise_clip": 0.5,
      "noise_clip_final": 0.2,
      "exploration_noise": 0.00,
      "exploration_noise_final": 0.00,
      "anneal_episodes": 80,
      "update_every": 2,
      "utd": 1,
      "max_updates_per_step": 1,
      "critic_pretrain_steps": 100000,
      "pretrain_noise": 0.00,
      "lambda_bc": 1.5,
      "lambda_bc_final": 0.1,
      "warmup_episodes": 20,
      "eval_every": 1,
      "use_huber": true,
      "huber_delta": 1.0,
      "max_grad_norm": 10.0,
      "seed": 42
    }
  }
}
